{
  "env": {
    "production": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "import",
          { "libraryName": "antd", "libraryDirectory": "es", "style": true }
        ]
      ]
    },
    "development": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "import",
          { "libraryName": "antd", "libraryDirectory": "es", "style": true }
        ]
      ]
    },
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ]
      ]
    }
  }
}
